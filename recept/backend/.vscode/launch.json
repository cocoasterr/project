{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8888",
        "--reload"
      ]
    },
    {
      "name": "FastAPI(WORKERS)",
      "type": "python",
      "request": "launch",
      "module": "hypercorn",
      "args": ["app.main:app", "--workers", "4", "--bind", "0.0.0.0:8000"]
    }
  ]
}
